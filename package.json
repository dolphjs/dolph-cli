{
  "name": "@dolphjs/cli",
  "version": "1.2.3",
  "description": "The dolphjs-cli is a tool for efficient initialization and management of a dolphjs project",
  "main": "./dist/index.js",
  "repository": "https://github.com/dolphjs/dolph-cli.git",
  "author": "<https://github.com/Xlaez>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint .",
    "check-lint": "eslint [0-9]*.ts",
    "clean": "rimraf ./dist",
    "build": "tsc ./lib/index.ts --esModuleInterop true --target ES2020 --module nodenext --moduleResolution nodenext --outDir dist",
    "start": "npm run clean && npm run build  && node ./dist/index.js",
    "dev": "ts-node ./lib/index.ts",
    "dev:start": "node ./dist/index.js",
    "pub": "npm run clean && npm run build"
  },
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "Dolphjs-cli",
    "Dolphjs-cli",
    "Dolphjs",
    "Dolph",
    "NodeJs",
    "cli"
  ],
  "files": [
    "dist/**/*"
  ],
  "bin": {
    "dolph": "./dist/index.js",
    "dc": "./dist/index.js"
  },
  "peerDependencies": {
    "ts-node": "^10.9.2",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.91"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "chokidar": "^3.5.3",
    "commander": "^11.1.0",
    "cross-spawn": "^7.0.3",
    "figlet": "^1.7.0",
    "inquirer": "^9.2.12",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "rimraf": "^5.0.5"
  },
  "devDependencies": {
    "@types/figlet": "^1.5.8",
    "@types/inquirer": "^9.0.7",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.9.1",
    "eslint": "^8.54.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "ts-node": "^10.9.2",
    "tslint": "^6.1.3",
    "typescript": "^5.2.2"
  }
}
